#自动开启TLS
veekxt.com {
    tls veekxt@gmail.com
    root * /var/www/
    file_server

    reverse_proxy /veekxt_h2_test 127.0.0.1:44222 {
        transport http {
          versions h2c
        }
    }
}

#手动开启TLS
veekxt.com {
    tls /etc/ssl/veekxt.com.cert /etc/ssl/veekxt.com.key
    root * /var/www/
    file_server

    reverse_proxy /veekxt_h2_test 127.0.0.1:44222 {
        transport http {
          versions h2c
        }
    }
}

#以上二选一即可
